<!DOCTYPE html>
<html>

<head>
  <!-- Basic -->
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <!-- Mobile Metas -->
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
  <!-- Site Metas -->
  <meta name="keywords" content="" />
  <meta name="description" content="" />
  <meta name="author" content="" />

  <title>Frontened Website</title>

  <!-- slider stylesheet -->
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" />
  <!-- bootstrap core css -->
  <link rel="stylesheet" type="text/css" href="/customerPublic/css/bootstrap.css" />
  <!-- font awesome style -->
  <link rel="stylesheet" type="text/css" href="/customerPublic/css/font-awesome.min.css" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <!-- Custom styles for this template -->
  <link href="/customerPublic/css/style.css" rel="stylesheet" />
  <!-- responsive style -->
  <link href="/customerPublic/css/responsive.css" rel="stylesheet" />


  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>
  


</head>



<section class="about_section layout_padding-bottom">
    <div class="container">
        <div class="row">
    
            <% let count = 0; 
            if((data && data.length > 0)) { %>
    
                <% data.forEach(function(item) { 
                    if(item.status != 0) { %>
    
                        <div class="col-lg-5 col-md-6">
                            <div class="detail-box">
                                <h2>
                                    <%= item.title %>
                                </h2>
                                <p>
                                    <%= item.description %>
                                </p>
                                <a href="https://en.wikipedia.org/wiki/Service_(economics)#:~:text=A%20service%20is%20an%20act,as%20a%20whole%20pays%20for.">
                                    Read More
                                </a>
                            </div>
                        </div>
                    
                        <div class="col-lg-7 col-md-6" style="margin-top: 40px;">
                            <div class="img-box">
                                <img src="/<%= item.image %>" alt="" class="img-fluid">
                            </div>
                        </div>
    
                <% } 
                }); %>
    
            <% } else { %>
                <h1>No data found</h1>
            <% } %>
    
        </div>
    </div>
    
</section>