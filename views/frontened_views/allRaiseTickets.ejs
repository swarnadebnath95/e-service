</head>

<div>
    <nav class="navstyle">
        <div class="navoption">
            <a href="" class="calling">
                <i class="fa fa-phone" aria-hidden="true" style="color: rgb(230, 93, 43);"></i>
                <span>
                    Call : +01 123455678990
                </span>
            </a>
            <a href="" class="email">
                <i class="fa fa-envelope" aria-hidden="true" style="color: rgb(230, 93, 43);"></i>
                <span>
                    Email : demo@gmail.com
                </span>
            </a>

            <a href="/frontened/account" class="logincus">
                <i class="fa fa-sign-in" aria-hidden="true" style="color: rgb(230, 93, 43);"></i>
                <span >
                   <a href="/frontened/account" style="color: white;text-decoration: none;"> back</a>
                </span>
            </a>

        </div>
    </nav>
</div>


<section style="padding-top: 80px;">

    <div class="container">
        <h4 class="ticketheader">Your All Complete Tickets</h4>
        
        <% if (tickets && tickets.length > 0) { %>
            <table class="table table-hover" style="margin-top: 40px;">
                <thead>
                    <tr>
                        <th style="color: rgb(67, 67, 173);">#</th>
                        <th style="color: rgb(67, 67, 173);">Ticket ID</th>
                        <th style="color: rgb(67, 67, 173);">Category</th>
                        <th style="color: rgb(67, 67, 173);">Model Number</th>
                        <th style="color: rgb(67, 67, 173);">Issue</th>
                        <th style="color: rgb(67, 67, 173);">Created At</th>
                    </tr>
                </thead>
                <tbody>
                    <% tickets.forEach(function(ticket, index) { %>
                        <tr>
                            <td><%= index + 1 %></td>
                            <td><%= ticket._id %></td>
                            <td><%= ticket.category %></td>
                            <td><%= ticket.category_model_number %></td>
                            <td><%= ticket.product_issue %></td>
                            <td><%= ticket.createdAt.toLocaleString() %></td>
                           
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        <% } else { %>
            <p>No tickets found.</p>
        <% } %>
    </div>
  

</section>