    <style>
        .navstyle {
            background-color: black;
            height: 50px;
        }

        .navoption {
            padding-top: 10px;
        }

        .calling {
            color: white;
            text-decoration: none;
            margin-left: 30px;
        }

        .email {
            color: white;
            text-decoration: none;
            margin-left: 360px;
        }

        .logincus {
            color: white;
            text-decoration: none;
            margin-left: 460px;
        }

        .update {
            width: 130px;
            height: 40px;
            margin-top: 30px;
            border-color: blue;
            color: rgb(43, 43, 177);
            font-weight: 700;
        }

        .update:hover {
            border-color: rgb(45, 45, 201);
            color: rgb(255, 255, 255);
